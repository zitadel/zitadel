    "startCommand": "java -Dspring.security.oauth2.client.provider.zitadel.issuer-uri={{ZITADEL_ISSUER}} -Dspring.security.oauth2.client.registration.zitadel.client-id={{ZITADEL_CLIENT_ID}} -jar web/target/web-0.0.2-SNAPSHOT.jar",
    "localhost": "http://localhost:18080/",
    "external": false,
    "envSetup": {
      "type": "command-args",
      "description": "Spring Boot uses command-line arguments for configuration. Replace the placeholders with your ZITADEL instance values.",
      "variables": [
        {
          "name": "ZITADEL_ISSUER",
          "description": "Your ZITADEL instance URL (e.g., https://my-domain.zitadel.cloud)",
          "placeholder": "https://{{ZITADEL_DOMAIN}}",
          "required": true
        },
        {
          "name": "ZITADEL_CLIENT_ID",
          "description": "Your application's client ID from ZITADEL",
          "placeholder": "{{CLIENT_ID}}@{{PROJECT_NAME}}",
          "required": true
        }
      ]
    }
