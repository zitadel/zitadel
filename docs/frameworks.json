  {
    "id": "flutter",
    "title": "Flutter",
    "ecosystem": "Mobile",
    "description": "Flutter example app (Android/iOS/Web)",
    "imgSrcDark": "/docs/img/tech/flutter.svg",
    "imgSrcLight": "/docs/img/tech/flutter.svg",
    "docsLink": "/docs/sdk-examples/flutter",
    "example": "https://github.com/zitadel/zitadel_flutter.git",
    "installCommand": "if cd flutter; then git pull && cd .. ; else git clone https://github.com/flutter/flutter.git; fi && ls && flutter/bin/flutter doctor && flutter/bin/flutter clean && flutter/bin/flutter config --enable-web",
    "startCommand": "flutter run -d chrome --web-port=4444 --dart-define zitadel_url={{ZITADEL_ISSUER}} --dart-define zitadel_client_id={{ZITADEL_CLIENT_ID}}",
    "localhost": "http://localhost:4444",
    "excludeFromAppCreation": true,
    "external": false,
    "envSetup": {
      "type": "command-args",
      "description": "Flutter uses --dart-define arguments for configuration. Replace the placeholders with your ZITADEL instance values.",
      "variables": [
        {
          "name": "ZITADEL_ISSUER",
          "description": "Your ZITADEL instance URL",
          "placeholder": "https://{{ZITADEL_DOMAIN}}",
          "required": true
        },
        {
          "name": "ZITADEL_CLIENT_ID",
          "description": "Your application's client ID from ZITADEL",
          "placeholder": "{{CLIENT_ID}}",
          "required": true
        }
      ]
    }
  },
