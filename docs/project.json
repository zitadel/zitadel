{
  "name": "@zitadel/docs",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "pnpm generate",
          "next build"
        ],
        "parallel": false,
        "cwd": "docs"
      },
      "outputs": [
        "{projectRoot}/.next"
      ]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next dev",
        "cwd": "docs"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next start",
        "cwd": "docs"
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm generate",
        "cwd": "docs"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint .",
        "cwd": "docs"
      }
    }
  }
}
