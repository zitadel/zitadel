{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "generate": {
      "inputs": [
        "default",
        "{workspaceRoot}/pnpm-lock.yaml",
        "{workspaceRoot}/proto/**/*"
      ],
      "outputs": [
        "{projectRoot}/cjs",
        "{projectRoot}/es",
        "{projectRoot}/types"
      ]
    },
    "build": {
      "description": "Generates protobuf files (alias for generate)",
      "dependsOn": ["generate"],
      "cache": true
    }
  }
}
